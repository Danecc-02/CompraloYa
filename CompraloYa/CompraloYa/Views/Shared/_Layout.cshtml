<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CompraYa</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel=”icon icon" type="image/x-icon" href=”CompraYa.ico” />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
           
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">
                
                        <img src="https://cdn.discordapp.com/attachments/811795660449775647/875942273936936961/Compraya1.png" width="166" height="45" />
                   
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        @*<li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>*@
                        <ul class="navbar-nav flex-grow-1">


                            <li class="nav-link text-dark dropdown ">
                                <a href="#" class="btn btn-outline-secondary dropdown-toggle " data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fas fa-bars"></i>Categorías</a>
                                <ul class="dropdown-menu dropdown-menu-dark">
                                    <li class="nav-item btn-dark">
                                        <a class="nav-link text-white" asp-area="" asp-controller="Joyerias" asp-action="Index"> Joyeria <i class="fas fa-clipboard-list"></i></a>
                                    </li>

                                    <li class="nav-item btn-dark">
                                        <a class="nav-link text-white" asp-area="" asp-controller="Oficinas" asp-action="Index"> Oficina <i class="fas fa-thumbtack"></i></a>
                                    </li>

                                    <li class="nav-item btn-dark">
                                        <a class="nav-link text-white" asp-area="" asp-controller="Ropas" asp-action="Index"> Ropa <i class="fas fa-bell"></i></a>
                                    </li>

                                    <li class="nav-item btn-dark">
                                        <a class="nav-link text-white" asp-area="" asp-controller="Tecnologias" asp-action="Index"> Tecnologia <i class="fas fa-bell"></i></a>
                                    </li>



                                </ul>


                                
                            </li>


                            <li class="nav-link text-dark dropdown ">
                                <a href="#" class="btn btn-outline-secondary dropdown-toggle " data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fas fa-bars"></i>Tipos</a>
                                <ul class="dropdown-menu dropdown-menu-dark">
                                    <li class="nav-item btn-dark">
                                        <a class="nav-link text-white" asp-area="" asp-controller="TypeRopas" asp-action="Index"> Tipos de ropa <i class="fas fa-clipboard-list"></i></a>
                                    </li>

                                    <li class="nav-item btn-dark">
                                        <a class="nav-link text-white" asp-area="" asp-controller="TypeSends" asp-action="Index"> Tipos de envio <i class="fas fa-thumbtack"></i></a>
                                    </li>

                                    <li class="nav-item btn-dark">
                                        <a class="nav-link text-white" asp-area="" asp-controller="TypeTecnologias" asp-action="Index"> Tipos de tecnologia <i class="fas fa-bell"></i></a>
                                    </li>

                                 



                                </ul>


                               
                            </li>

                        </ul>
                        
                       
                        @*<li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </li>*@
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - CompraloYa - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
